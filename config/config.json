{	"setup":{
		"ros2": {
			"ros2Repo_0":{
                                "repo": "git@github.com:rudislabs/tflite_msgs.git",
                                "version": "humble",
                                "build": {
                                        "package": "tflite_msgs",
                                        "prefix": "--packages-select",
                                        "postfix": "--symlink-install"
                                }
                        },
			"ros2Repo_1":{
				"repo": "git@github.com:rudislabs/canfd_msgs.git",
				"version": "humble",
				"build": {
					"package": "canfd_msgs",
					"prefix": "--packages-select",
					"postfix": "--symlink-install"
				}
			},
			"ros2Repo_2":{
                                "repo": "git@github.com:rudislabs/nxp_cv_cam.git",
                                "version": "humble",
                                "build": {
                                        "package": "nxp_cv_cam",
                                        "prefix": "--packages-select",
                                        "postfix": "--symlink-install"
                                }
                        },
			"ros2Repo_3":{
                                "repo": "git@github.com:rudislabs/nxp_tf_lite.git",
                                "version": "dev",
                                "build": {
                                        "package": "nxp_tf_lite",
                                        "prefix": "--packages-select",
                                        "postfix": "--symlink-install"
                                }
                        },
                        "ros2Repo_4":{
                                "repo": "git@github.com:rudislabs/ucans32k1_spi_led.git",
                                "version": "humble",
                                "build": {
                                        "package": "ucans32k1_spi_led",
                                        "prefix": "--packages-select",
                                        "postfix": "--symlink-install"
                                }
                        },
			"ros2Repo_5":{
				"repo": "git@github.com:rudislabs/nxp_cup_vision.git",
				"version": "aim_ign",
				"build": {
					"package": "nxp_cup_vision",
					"prefix": "--packages-select",
					"postfix": "--symlink-install"
				}
			}
		}
	},
	"nodes": {
		"ros2Node_0":{
			"package": "nxp_cv_cam",
			"executable": "nxp_cv_cam_node",
			"name": "nxp_cv_cam_node_0",
			"output": "screen",
			"parameters": [	{"camera_topic": "/NPU/image_real"},
							{"device": "/dev/video3"},
							{"framerate": 15},
							{"resolution": [640, 480]}
						]
		},
		"ros2Node_1":{
                        "package": "nxp_tf_lite",
                        "executable": "nxp_tf_lite_node",
                        "name": "nxp_tf_lite_node_0",
                        "output": "screen",
                        "parameters": [ {"camera_image_0": "/NPU/image_real"},
                                        {"camera_image_1": "/NPU/image_sim"},
                                                        {"model_file": "mobilenet_ssd_v2_coco_quant_postprocess.tflite"},
                                                        {"label_file": "coco.txt"},
                                                        {"ext_delegate": "/usr/lib/libvx_delegate.so"},
							{"ext_delegate_options": ""},
                                                        {"topic_name_0": "TFLiteReal"},
                                                        {"topic_name_1": "TFLiteSim"},
                                                        {"gpu_inference": false},
							{"bbox_index": 0},
                                                        {"score_index": 2},
                                                        {"class_index": 1}
                                                ]
                },
		"ros2Node_2":{
                        "package": "ucans32k1_spi_led",
                        "executable": "ucans32k1_spi_led_node",
                        "name": "ucans32k1_spi_led_node_0",
                        "output": "screen",
                        "parameters": [{"led_brightness": 1}]
                },
		"ros2Node_3":{
			"package": "nxp_cup_vision",
			"executable": "nxp_track_vision",
			"name": "nxp_cup_vision_node_sim",
			"output": "screen",
			"parameters": [{"debug_image": "/DebugCupSim"},
							{"cmd_topic": "/requested_vel"},
							{"camera_image": "/NPU/image_sim"},
							{"linear_velocity": 1.25},
							{"debug": false}]
		}
	}
}
