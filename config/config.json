{        "setup":{
                "ros2": {
                        "ros2Repo_0":{
                                "repo": "git@github.com:rudislabs/tflite_msgs.git",
                                "version": "humble",
                                "build": {
                                        "package": "tflite_msgs",
                                        "prefix": "--packages-select",
                                        "postfix": "--symlink-install"
                                }
                        },
                        "ros2Repo_1":{
                                "repo": "git@github.com:rudislabs/debug_image_tf_lite.git",
                                "version": "humble",
                                "build": {
                                        "package": "debug_image_tf_lite",
                                        "prefix": "--packages-select",
                                        "postfix": "--symlink-install"
                                }
                        }
                }
        },
        "nodes": {
                "ros2Node_0":{
                        "package": "debug_image_tf_lite",
                        "executable": "debug_image_tf_lite_node",
                        "name": "debug_image_tf_lite_node_real",
                        "output": "screen",
                        "parameters": [{"debug_topic": "/CSI2/image_tf"},
                                                        {"tflite_topic": "/CSI2/TFLite"},
                                                        {"image_topic": "/CSI2/image_raw"},
                                                        {"threshold": 0.55},
                                                        {"max_returns": 5},
                                                        {"max_queue": 30}]
                },
                "ros2Node_1":{
                        "package": "rqt_image_view",
                        "executable": "rqt_image_view",
                        "name": "rqt_image_view_0",
                        "output": "screen",
                        "parameters": []
                }
        }
}
